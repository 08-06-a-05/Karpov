import sqlite3


def initialization():
    connection = sqlite3.connect('NewDB.db')
    cursor = connection.cursor()
    return connection, cursor


connection, cursor = initialization()
cursor.execute('SELECT id_car FROM cars WHERE brand=(SELECT id FROM brands WHERE name="Renault")')
print(cursor.fetchall())
cursor.execute(
    'SELECT name_of_customer AS "Имя клиента",phone AS "Телефон клиента" FROM orders WHERE id_car=(SELECT id_car FROM cars WHERE brand=(SELECT id FROM brands WHERE name="BMW") AND model="X5")')
print(cursor.fetchall())
cursor.execute(
    'SELECT name_of_customer AS "Имя клиента",phone AS "Телефон клиента" FROM orders WHERE status="Интересовался" AND best_price>=10000000000 AND id_car=(SELECT id_car FROM cars where brand=(SELECT id from brands where name="BMW") and model="X5")')
print(cursor.fetchall())
connection.close()
